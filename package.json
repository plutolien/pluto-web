{
  "name": "pluto-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm dev",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:next": "next dev",
    "dev:tailwind": "npx tailwindcss -i src/styles/style.css -o src/styles/build.css --minify --watch",
    "build": "npm build:tailwind && npm build:next",
    "build:next": "next build",
    "build:tailwind": "npx tailwindcss -i src/styles/style.css -o src/styles/build.css --minify",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@iconify/icons-heroicons-solid": "^1.2.1",
    "@react-icons/all-files": "^4.1.0",
    "heroicons-react": "^1.4.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-scroll": "^1.8.7",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "concurrently": "^7.2.1",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.7.2"
  }
}
